{
  "name": "tuvi-neo",
  "version": "1.0.7",
  "description": "Vietnamese Ziwei Doushu (Tử Vi Đẩu Số) natal chart calculation library",
  "repository": {
    "type": "git",
    "url": "https://github.com/implicit-invocation/tuvi-neo.git"
  },
  "homepage": "https://github.com/implicit-invocation/tuvi-neo#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts && bun run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "tuvi",
    "ziwei-doushu",
    "vietnamese-astrology",
    "natal-chart",
    "tu-vi-dau-so"
  ],
  "dependencies": {
    "@dqcai/vn-lunar": "^1.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
